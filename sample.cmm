func sum(a: real, b: real) -> real {
    var c = a; # type infered
    {
        # nested scope
        c += b;
    }
    return c;
}

func feb(a: real) -> real {
    # recursive functions :)
    if (a <= 1)
        return a;
    return  feb(a - 1) + feb(a - 2);
}

func inc(a: real&) -> void {
    a++;
}

